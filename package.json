{
  "name": "Frontfriend",
  "version": "1.0.0",
  "description": "An easy to use auto-deployer for nodejs web projects",
  "main": "./dist/index.js",
  "author": "Matthew Wareing",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prestart": "tsc --build",
    "start": "node .",
    "dev": "tsc --build && concurrently -n node,ts \"nodemon dist/index.js\" \"tsc --watch\""
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.3",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "typescript": "^4.7.4"
  },
  "volta": {
    "node": "16.16.0",
    "yarn": "1.22.19"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "reflect-metadata": "^0.1.13",
    "simple-git": "^3.11.0",
    "sqlite3": "^5.0.11",
    "typeorm": "^0.3.7"
  }
}
